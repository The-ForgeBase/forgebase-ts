{
  "name": "@forgebase-ts/api",
  "version": "0.0.1",
  "private": false,
  "type": "commonjs",
  "main": "./src/index.js",
  "module": "./dist/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./frameworks": {
      "types": "./dist/frameworks/index.d.ts",
      "import": "./dist/frameworks/index.js",
      "default": "./dist/frameworks/index.js",
      "require": "./dist/frameworks/index.js"
    },
    "./frameworks/nest": {
      "types": "./dist/frameworks/nest/index.d.ts",
      "import": "./dist/frameworks/nest/index.js",
      "default": "./dist/frameworks/nest/index.js",
      "require": "./dist/frameworks/nest/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js",
      "default": "./dist/adapters/index.js",
      "require": "./dist/adapters/index.js"
    },
    "./core/nest": {
      "types": "./dist/core/nest/index.d.ts",
      "import": "./dist/core/nest/index.js",
      "default": "./dist/core/nest/index.js",
      "require": "./dist/core/nest/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js",
      "default": "./dist/core/index.js",
      "require": "./dist/core/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "frameworks": [
        "./dist/frameworks/index.d.ts"
      ],
      "frameworks/nest": [
        "./dist/frameworks/nest/index.d.ts"
      ],
      "adapters": [
        "./dist/adapters/index.d.ts"
      ],
      "core/nest": [
        "./dist/core/nest/index.d.ts"
      ],
      "core": [
        "./dist/core/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "express": "^4.21.2",
    "fastify": "^5.2.1",
    "hono": "^4.6.19",
    "@forgebase-ts/database": "0.0.1",
    "@nestjs/common": "^10.0.2",
    "rou3": "^0.5.1",
    "knex": "^3.1.0",
    "@libsql/knex-libsql": "^0.1.0",
    "@nestjs/core": "^10.0.2",
    "@forgebase-ts/storage": "0.0.1",
    "better-auth": "^1.1.14"
  }
}
