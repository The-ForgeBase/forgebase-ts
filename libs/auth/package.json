{
  "name": "@forgebase-ts/auth",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    },
    "./adapters": {
      "types": "./src/adapters/index.d.ts",
      "import": "./src/adapters/index.js",
      "require": "./src/adapters/index.js",
      "default": "./src/adapters/index.js"
    },
    "./adapters/nest": {
      "types": "./src/adapters/nest/index.d.ts",
      "import": "./src/adapters/nest/index.js",
      "require": "./src/adapters/nest/index.js",
      "default": "./src/adapters/nest/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.d.ts"
      ],
      "adapters": [
        "./src/adapters/index.d.ts"
      ],
      "adapters/nest": [
        "./src/adapters/nest/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "express": "^4.21.2",
    "fastify": "^5.2.1",
    "@fastify/cookie": "^11.0.2",
    "hono": "^4.6.19",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "knex": "^3.1.0",
    "@oslojs/crypto": "^1.0.1",
    "rfc4648": "^1.5.4",
    "@node-rs/argon2": "^2.0.2",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.4",
    "twilio": "^5.4.2",
    "nodemailer": "^6.10.0",
    "arctic": "^3.2.1",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "@forgebase-ts/common": "0.0.1",
    "zod": "^3.24.1",
    "itty-router": "^5.0.18",
    "@forgebase-ts/database": "0.0.1",
    "jsx-email": "^2.7.1",
    "react": "18.3.1",
    "uuid": "^11.1.0",
    "jose": "^5.9.6",
    "tsup": "^8.4.0",
    "tslib": "^2.4.0"
  }
}
