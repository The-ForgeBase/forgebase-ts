{
  "name": "@forgebase-ts/auth",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js",
      "require": "./dist/adapters/index.js",
      "default": "./dist/adapters/index.js"
    },
    "./adapters/nest": {
      "types": "./dist/adapters/nest/index.d.ts",
      "import": "./dist/adapters/nest/index.js",
      "require": "./dist/adapters/nest/index.js",
      "default": "./dist/adapters/nest/index.js"
    },
    "./adapters/web": {
      "types": "./dist/adapters/web/index.d.ts",
      "import": "./dist/adapters/web/index.js",
      "require": "./dist/adapters/web/index.js",
      "default": "./dist/adapters/web/index.js"
    },
    "./adapters/express": {
      "types": "./dist/adapters/express/index.d.ts",
      "import": "./dist/adapters/express/index.js",
      "require": "./dist/adapters/express/index.js",
      "default": "./dist/adapters/express/index.js"
    },
    "./adapters/ultimate-express": {
      "types": "./dist/adapters/ultimate-express/index.d.ts",
      "import": "./dist/adapters/ultimate-express/index.js",
      "require": "./dist/adapters/ultimate-express/index.js",
      "default": "./dist/adapters/ultimate-express/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "express": "^4.21.2",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "knex": "^3.1.0",
    "@oslojs/crypto": "^1.0.1",
    "rfc4648": "^1.5.4",
    "@node-rs/argon2": "^2.0.2",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.4",
    "twilio": "^5.4.2",
    "nodemailer": "^6.10.0",
    "arctic": "^3.2.1",
    "axios": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "@forgebase-ts/common": "0.0.1",
    "zod": "^3.24.1",
    "itty-router": "^5.0.18",
    "@forgebase-ts/database": "0.0.1",
    "jsx-email": "^2.7.1",
    "react": "18.3.1",
    "uuid": "^11.1.0",
    "tsup": "^8.4.0",
    "tslib": "^2.4.0",
    "awilix": "^12.0.5",
    "ultimate-express": "^1.4.6",
    "knex-mock-client": "^3.0.2"
  },
  "peerDependencies": {
    "@forgebase-ts/database": "0.0.1",
    "@forgebase-ts/common": "0.0.1"
  }
}
