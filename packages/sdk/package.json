{
  "name": "@forgebase/sdk",
  "version": "0.0.4",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsup --format esm --outdir dist/esm",
    "build:cjs": "tsup --format cjs --outdir dist/cjs",
    "build": "pnpm clean && tsup",
    "dev": "pnpm build:esm --watch",
    "lint": "tsc --noEmit"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/database/client/index.d.ts",
        "default": "./dist/database/client/index.js"
      },
      "require": {
        "types": "./dist/database/client/index.d.cts",
        "default": "./dist/database/client/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/database/server/index.d.ts",
        "default": "./dist/database/server/index.js"
      },
      "require": {
        "types": "./dist/database/server/index.d.cts",
        "default": "./dist/database/server/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "client": [
        "./dist/database/client/index.d.ts"
      ],
      "server": [
        "./dist/database/server/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "package.json",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@forgebase/database": "workspace:*",
    "axios": "^1.7.9",
    "kysely": "^0.28.11"
  },
  "peerDependencies": {
    "@forgebase/database": "workspace:*"
  },
  "devDependencies": {
    "@forgebase/typescript-config": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "tslib": "^2.3.0",
    "tsup": "^8.5.1",
    "typescript": "5.8.2"
  }
}
