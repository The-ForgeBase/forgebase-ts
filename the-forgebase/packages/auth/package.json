{
  "name": "@the-forgebase/auth",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "unbuild",
    "lint": "tsc --noEmit"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./adapters/nest": {
      "types": "./dist/adapters/nest/index.d.ts",
      "import": "./dist/adapters/nest/index.mjs",
      "require": "./dist/adapters/nest/index.cjs"
    },
    "./adapters/web": {
      "types": "./dist/adapters/web/index.d.ts",
      "import": "./dist/adapters/web/index.mjs",
      "require": "./dist/adapters/web/index.cjs"
    },
    "./adapters/express": {
      "types": "./dist/adapters/express/index.d.ts",
      "import": "./dist/adapters/express/index.mjs",
      "require": "./dist/adapters/express/index.cjs"
    },
    "./adapters/ultimate-express": {
      "types": "./dist/adapters/ultimate-express/index.d.ts",
      "import": "./dist/adapters/ultimate-express/index.mjs",
      "require": "./dist/adapters/ultimate-express/index.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@the-forgebase/typescript-config": "workspace:*",
    "tsc": "^2.0.4",
    "tslib": "^2.8.1",
    "unbuild": "^3.5.0",
    "typescript": "latest"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "arctic": "^3.6.1",
    "awilix": "^12.0.5",
    "axios": "^1.9.0",
    "express": "^5.1.0",
    "itty-router": "^5.0.18",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "knex-mock-client": "^3.0.2",
    "nodemailer": "^7.0.3",
    "qrcode": "^1.5.4",
    "rfc4648": "^1.5.4",
    "speakeasy": "^2.0.0",
    "twilio": "^5.6.0",
    "ultimate-express": "^1.4.9",
    "uuid": "^11.1.0",
    "zod": "^3.24.4",
    "@the-forgebase/common": "workspace:*"
  }
}
