{
  "name": "@the-forgebase/api",
  "version": "0.0.1",
  "private": false,
  "type": "module",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js",
      "default": "./src/index.js"
    },
    "./core/nest": {
      "types": "./src/core/nest/index.d.ts",
      "import": "./src/core/nest/index.js",
      "require": "./src/core/nest/index.js",
      "default": "./src/core/nest/index.js"
    },
    "./core/web": {
      "types": "./src/core/web/index.d.ts",
      "import": "./src/core/web/index.js",
      "require": "./src/core/web/index.js",
      "default": "./src/core/web/index.js"
    },
    "./core/express": {
      "types": "./src/core/express/index.d.ts",
      "import": "./src/core/express/index.js",
      "require": "./src/core/express/index.js",
      "default": "./src/core/express/index.js"
    },
    "./core/ultimate-express": {
      "types": "./src/core/ultimate-express/index.d.ts",
      "import": "./src/core/ultimate-express/index.js",
      "require": "./src/core/ultimate-express/index.js",
      "default": "./src/core/ultimate-express/index.js"
    },
    "./core": {
      "types": "./src/core/index.d.ts",
      "import": "./src/core/index.js",
      "require": "./src/core/index.js",
      "default": "./src/core/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./src/index.d.ts"
      ],
      "frameworks": [
        "./src/frameworks/index.d.ts"
      ],
      "frameworks/nest": [
        "./src/frameworks/nest/index.d.ts"
      ],
      "core/nest": [
        "./src/core/nest/index.d.ts"
      ],
      "core/hono": [
        "./src/core/hono/index.d.ts"
      ],
      "core/web": [
        "./src/core/web/index.d.ts"
      ],
      "core": [
        "./src/core/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "express": "^4.21.2",
    "@nestjs/common": "^10.0.2",
    "@the-forgebase/database": "workspace:*",
    "@the-forgebase/storage": "workspace:*",
    "knex": "^3.1.0",
    "@nestjs/core": "^10.0.2",
    "itty-router": "^5.0.18",
    "@the-forgebase/auth": "workspace:*",
    "tsup": "^8.4.0",
    "ultimate-express": "^1.4.6"
  },
  "peerDependencies": {
    "@the-forgebase/database": "workspace:*",
    "@the-forgebase/storage": "workspace:*",
    "@the-forgebase/auth": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@the-forgebase/typescript-config": "workspace:*"
  }
}
